{
  "Version":"2012-10-17",
  "Statement":[
    {
      "Sid":"DenyAllOutsideCanadaUS",
      "Effect":"Deny",
      "NotAction":[
        "a4b:*",
        "aws-marketplace-management:*",
        "aws-marketplace:*",
        "aws-portal:*",
        "awsbillingconsole:*",
        "budgets:*",
        "ce:*",
        "chime:*",
        "cloudfront:*",
        "config:*",
        "cur:*",
        "ec2:DescribeRegions",
        "fms:*",
        "globalaccelerator:*",
        "health:*",
        "iam:*",
        "importexport:*",
        "mobileanalytics:*",
        "organizations:*",
        "pricing:*",
        "route53:*",
        "route53domains:*",
        "s3:GetAccountPublic*",
        "s3:PutAccountPublic*",
        "s3:listBuckets",
        "shield:*",
        "sts:*",
        "support:*",
        "trustedadvisor:*",
        "waf-regional:*",
        "waf:*",
        "wafv2:*",
        "wellarchitected:*"
      ],
      "Resource":"*",
      "Condition":{
        "StringNotEquals":{
          "aws:RequestedRegion":[
            "ca-central-1",
            "us-east-1",
            "us-east-2",
            "us-west-1",
            "us-west-2"
          ]
        },
        "ArnNotLike":{
          "aws:PrincipalARN":[
            "arn:aws:iam::*:role/AWSCloudFormationStackSetExecutionRole",
            "arn:aws:iam::*:role/PBMMAccel-*"
          ]
        }
      }
    }
  ]
}